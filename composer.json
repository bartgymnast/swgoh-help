{
    "name": "matthillman/swgoh-help",
    "description": "Laravel wrapper around an API to swogh.help.",
    "keywords": ["swogh", "swgoh.help", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Hillman",
            "email": "mhillman@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "fabpot/goutte": "^3.2",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "~5.4",
        "myclabs/php-enum": "^1.6",
        "salsify/json-streaming-parser": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "SwgohHelp\\": "src/"
        },
		"files": [
			"src/Support/helpers.php"
		]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwgohHelp\\SwgohHelpServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
